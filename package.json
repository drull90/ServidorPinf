{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "npm run ng-build && firebase emulators:start --import=./DataBase --export-on-exit --only firestore,functions,hosting",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run ng-build-prod && firebase deploy --only functions,firestore,auth,hosting",
    "logs": "firebase functions:log",
    "ng-build": "cd src/cliente && ng build && cd ../..",
    "ng-build-prod": "cd src/cliente && ng build --prod && cd ../.."
  },
  "engines": {
    "node": "12"
  },
  "main": "src/servidor/index.js",
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/fire": "^6.1.1",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/cdk": "^11.0.4",
    "@angular/forms": "11.0.2",
    "angular-file-uploader": "^7.0.3",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.6.1",
    "pdfreader": "^1.2.7",
    "rxjs": "~6.6.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "~11.0.0",
    "codelyzer": "^6.0.0",
    "eslint": "^5.12.0",
    "google-gax": "^2.9.2",
    "eslint-plugin-promise": "^4.0.1",
    "jasmine-core": "~3.6.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "private": true
}
